{
  "name": "react-wizard-simple",
  "version": "0.0.6",
  "description": "A super-fast, bare-bones package to get you started creating a wizard with React hooks.",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "README.md"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc",
    "deploy": "npm run build && npm publish",
    "lint": "tslint  --project tsconfig.json 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint  --project tsconfig.json 'src/**/*.ts'",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianyang/react-wizard-simple.git"
  },
  "keywords": [
    "react",
    "hooks",
    "wizard",
    "mult-step form"
  ],
  "author": "brianyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianyang/react-wizard-simple/issues"
  },
  "homepage": "https://github.com/brianyang/react-wizard-simple#readme",
  "dependencies": {
    "@types/react-dom": "^16.9.1",
    "react-dom": "^16.10.0"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-scripts": "^3.0.1",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "^3.2.4"
  }
}
